.title{
  text-align: left ;
  padding-left: 15rpx;
  font-size: 34rpx;
}
.title text{
  font-weight: bold;
  padding-right: 6rpx;
}
.product-display-ul{
  padding: 20rpx 0;
}
.main{text-align: center;margin: 0 10rpx;}
swiper {
    width: 100%;
    height: 500rpx;
    margin: 0 auto 20rpx auto;
}
swiper-item image {
     width: 100%;
     height: 100%;
}
.index-top{
  position: relative;
  text-align: center;
}
.index-top-list{
  position: absolute;
  top: 30rpx;
  width: 100%;
}
.index-top-one{
  font-size: 40rpx;
}
.index-top-two{
  margin: 20rpx 0;
}
.index-top-three{
  width: 200rpx;
  border-radius: 20rpx;
  margin: 0 auto;
}
.recom-top{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 20rpx;
}
.recom-top navigator{
  width: 49%;
}
.recom-top image{
  width:100%;
  height:270rpx;
  background-color:#eee;
  border:0.5px solid #f0eeee;
}
