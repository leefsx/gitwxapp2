/* page */
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  color: #383838;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.topbar {
  padding: 30rpx 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content:space-between;
  align-items: center;
}
.topbar-title image {
  width: 120rpx;
  height: 90rpx;
  vertical-align: top;
  line-height: 90rpx;
}
.top-tele image{
  width: 70rpx;
  height: 70rpx;
}
.topbar text {
  display: block;
  font-weight: bold; 
  margin-left: 10rpx;
}
.topbar-title>text{
  display: inline-block;
  font-size: 48rpx;
  line-height: 90rpx;
  color: #313131;
}
.top-tele{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.top-tele view text:first-child{
  color: #959595;
  margin-top: 10rpx;
  font-size: 24rpx;
}
