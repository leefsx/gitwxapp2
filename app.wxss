.product-nav:hover{
  border-color: #c71f3b;
}
.products text{
  display: block;
  text-align: left;
  font-size: 26rpx;
  padding-top: 6rpx;
  padding-left: 10rpx;
}
.product-name{
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.product-price{
  position: absolute;
  top: 0;
  left: 0;
  padding: 5rpx 10rpx;
}
.product-dec{
  color:#707070;
  padding-bottom:10rpx;
  width:100%;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}



