/*产品列表样式*/
.products{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}
.product-nav{
  width: 49%;
  border: 1rpx solid #f0eeee;
  box-sizing: border-box;
  margin-bottom: 15rpx;
  overflow: hidden;
  position: relative;
}






